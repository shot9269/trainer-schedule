<ul class = "client_info">
  <%= form_with model: @client, local: true do |f| %>
    <li>
      <%= f.label :start_time, "次回予約日" %><br>
      <%= f.date_field :start_time %>
    </li>
    <li>
      <%= f.label :session_time, "開始時間" %><br>
      <%= f.time_select :session_time%>
    </li>
    <%= f.submit "保存する" %>
  <%end%>
</ul>